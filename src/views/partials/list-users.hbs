<h2 class="subtitle">Users</h2>
<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>
        First name
      </th>
      <th>
        Last name
      </th>
      <th>
        Email
      </th>
    </tr>
  </thead>
  <tbody>
    {{#each this.users}}
      <tr>
        <td>
          {{firstName}}
        </td>
        <td>
          {{lastName}}
        </td>
        <td>
          {{email}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
